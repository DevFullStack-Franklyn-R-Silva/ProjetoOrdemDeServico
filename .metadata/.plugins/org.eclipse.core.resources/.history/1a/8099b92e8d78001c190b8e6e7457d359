package com.github.hadesfranklyn.domain;

public abstract class Pessoa {
	
	private Integer id;
	private String nome;
	private String cpf;
	private String telefone;
	
	public Pessoa() {
		super();
	}

	public Pessoa(Integer id, String nome, String cpf, String telefone) {
		super();
		this.id = id;
		this.nome = nome;
		this.cpf = cpf;
		this.telefone = telefone;
	}

	
	
	
}
